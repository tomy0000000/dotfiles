#!/bin/zsh

alias 'pip-s'='pip search'
alias 'pip-f'='pip show'
alias 'pip-i'='pip install'
alias 'pip-ir'='pip install -r requirements.txt'
alias 'pip-u'='pip uninstall'
alias 'pip-ur'='pip uninstall -y -r requirements.txt'
alias 'pip-ls'='pip list'
alias 'pip-fr'='pip freeze'
alias 'pip-frr'='pip freeze > requirements.txt'
